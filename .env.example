# Example environment configuration file
# Copy this file to .env and fill in real configuration information

# Aliyun OSS Configuration
OSS_ACCESS_KEY_ID=your_access_key_id
OSS_ACCESS_KEY_SECRET=your_access_key_secret
OSS_BUCKET=your_bucket_name
OSS_ENDPOINT=oss-cn-hangzhou.aliyuncs.com

# Tencent Cloud COS Configuration
COS_SECRET_ID=your_secret_id
COS_SECRET_KEY=your_secret_key
COS_BUCKET=your_bucket_name
COS_REGION=ap-guangzhou

# SM.MS Configuration
SMS_API_TOKEN=your_api_token

# Imgur Configuration
IMGUR_CLIENT_ID=your_client_id
IMGUR_CLIENT_SECRET=your_client_secret
IMGUR_ACCESS_TOKEN=your_access_token
IMGUR_REFRESH_TOKEN=your_refresh_token

# GitHub Configuration
GITHUB_TOKEN=your_github_token
GITHUB_OWNER=your_username
GITHUB_REPO=your_repo_name
GITHUB_PATH=images/